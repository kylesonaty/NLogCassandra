        <tags>NLog Cassandra DataStax</tags>
        <dependencies>
            <dependency id="NLog" version="3.1.0.0" />
            <dependency id="FSharp.Core" version="3.1.2.1" />
            <dependency id="CassandraCSharpDriver" version="2.5.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\NLogCassandra\bin\Release\NLogCassandra.dll" target="lib\NLogCassandra.dll" />
    </files>
</package>